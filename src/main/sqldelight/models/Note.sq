CREATE TABLE Note (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    title TEXT NOT NULL,
    body TEXT NOT NULL
);

CREATE INDEX title ON Note(title);

insert: INSERT INTO Note(title, body) VALUES (?, ?);

selectAll: SELECT * FROM Note;

selectNote: SELECT * FROM Note WHERE id = ?;

deleteNote: DELETE FROM Note WHERE id = ?;

updateNote: UPDATE Note SET title = ?, body = ? WHERE id = ?;